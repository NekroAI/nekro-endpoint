{
  "$schema": "https://raw.githubusercontent.com/cloudflare/wrangler/main/config-schema.json",
  "name": "nekro-endpoint",
  "main": "src/index.ts",
  "compatibility_date": "2024-07-29",
  "compatibility_flags": ["nodejs_compat"],
  "assets": {
    "binding": "ASSETS",
    "directory": "./dist/client"
  },
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "nekro-endpoint-db-local",
      "database_id": "local",
      "migrations_dir": "drizzle"
    }
  ],
  "vars": {
    "NODE_ENV": "development",
    "VITE_PORT": "5173",
    "APP_BASE_URL": "http://localhost:8787"
  },
  "env": {
    "production": {
      "assets": {
        "binding": "ASSETS",
        "directory": "./dist/client"
      },
      "d1_databases": [
        {
          "binding": "DB",
          "database_name": "nekro-endpoint-db",
          "database_id": "0c881890-ed00-4517-acb1-3bb275e36f03",
          "migrations_dir": "drizzle"
        }
      ],
      "vars": {
        "NODE_ENV": "production",
        "VITE_PORT": "5173",
        "APP_BASE_URL": "https://ep.nekro.ai"
      },"name": "nekro-endpoint"
    }
  }
}
